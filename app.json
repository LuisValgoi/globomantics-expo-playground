{
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "scheme": "globomantics",
    "name": "globomantics",
    "slug": "globomantics",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.globomantics.App"
    },
    "android": {
      "package": "com.globomantics.App",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "48.0.0",
    "extra": {
      "eas": {
        "projectId": "ea84b86a-c938-43e5-bc0a-e4ec178bb375"
      }
    }
  }
}
